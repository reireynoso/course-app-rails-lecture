<h1>Students New Form</h1>

<% if flash[:errors]%>
    <% flash[:errors].each do |error|%>
        <p style="color: red"><%= error%></p>
    <%end%>
<%end%>

<%= form_for @student do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name %>
    <%= f.label :age%>
    <%= f.number_field :age %>

    <%= f.submit%>
<%end%>
